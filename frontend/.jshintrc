{
  "browserify": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "expr" : true,
  "forin": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": false,
  "noarg": true,
  "node": true,
  "nonbsp": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "trailing": true,
  "globals": {
    "angular"   : true,
    "atob"      : false,
    "Blob"      : false,
    "document"  : false,
    "FormData"  : false,
    "URL"       : false
  }
}